{
  "version": "1.0",
  "manifest_version": 3,  
  "name": "Better Canvas Navigation Toggle",
  "description": "ASU student canvas navigation button visible after the page is scrolled",
    "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://*.asu.instructure.com/courses/*"],
      "exclude_matches": ["https://*.asu.instructure.com/courses/", "https://*.asu.instructure.com/courses"],
      "js": ["courseMenuToggle_AfterScroll.js"],
      "css": ["courseMenuToggle_AfterScroll.css"]
    }
    ],
    "action": {
      "default_icon": "images/CMT_AS_icon.png"
    },
    "author": {
      "email": "jmstanelle@hotmail.com"
    }
  }